<Page x:Class="TestWpf.Pages.AdminPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
       DataContext="{Binding AdminPage, Source={StaticResource Locator}}"
      xmlns:local="clr-namespace:MVVM"
      mc:Ignorable="d" 
      d:DesignHeight="600" d:DesignWidth="480"
      Title="Page2">

    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="37*"/>
            <ColumnDefinition Width="493*"/>
        </Grid.ColumnDefinitions>
        <Button Content="Show all groups" Grid.Column="1" HorizontalAlignment="Center" Margin="10,53,0,0" VerticalAlignment="Top" Width="108" Height="24"  Click="Button_Click_ToGroups"/>
        <Button Content="Show logs" Grid.Column="1" HorizontalAlignment="Center" Margin="10,105,0,0" VerticalAlignment="Top" Width="108" Height="24"  Click="Button_Click_ToLogs"/>
        <Button Content="Show all users" Grid.Column="1" HorizontalAlignment="Center" Margin="10,158,0,0" VerticalAlignment="Top" Width="108" Height="24"  Click="Button_Click_ToUsers"/>
        <Button Content="Back" Grid.Column="1" HorizontalAlignment="Center" Margin="10,211,0,0" VerticalAlignment="Top" Width="108" Height="24"  Click="Button_Click_GoBack"/>
    </Grid>
</Page>
